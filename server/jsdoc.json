{
    "source": {
        "include": [
            "index.js"
        ],
        "includePattern": ".js$",
        "excludePattern": "(nodes_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "template": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "destination": "./docs/"
    }
}